#!/bin/bash


IMAGE_NAME=deluge-ubuntu
Date=$( date +%Y-%m-%d )


docker image build --tag volaricsi/${IMAGE_NAME} --tag volaricsi/${IMAGE_NAME}:${Date} -f Dockerfile.ubuntu .
Ver=$( docker run --rm  -it volaricsi/${IMAGE_NAME} verzio )
docker tag volaricsi/${IMAGE_NAME} volaricsi/${IMAGE_NAME}:${Ver}


docker push 	volaricsi/${IMAGE_NAME}:${Date}
docker push 	volaricsi/${IMAGE_NAME}:${Ver}
docker push 	volaricsi/${IMAGE_NAME}
